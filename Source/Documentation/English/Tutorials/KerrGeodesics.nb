Cell[CellGroupData[{

Cell["Visualize the orbit", \
"Subsection",ExpressionUUID->"cf9c726b-e59c-4df3-bb12-58a2a02d9077"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"KerrEqProgradePlot", "[", 
    RowBox[{"a_", ",", "p_", ",", "e_", ",", "\[Chi]max_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "orbit", ",", "rmax", ",", "t", ",", "r", ",", "\[Theta]", ",", 
       "\[Phi]", ",", "stable"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"stable", "=", 
       RowBox[{"KerrGeoStableOrbitQ", "[", 
        RowBox[{"a", ",", "p", ",", "e", ",", "0"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"stable", "\[Equal]", "True"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"orbit", "=", 
          RowBox[{"KerrGeoOrbit", "[", 
           RowBox[{"a", ",", "p", ",", "e", ",", "0"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}], 
          "=", 
          RowBox[{"orbit", "[", "\"\<Interpolation\>\"", "]"}]}], ";"}]}], 
       RowBox[{"(*", 
        RowBox[{
        "The", " ", "interpolated", " ", "version", " ", "is", " ", "faster", 
         " ", "to", " ", "evaluate"}], "*)"}], "\[IndentingNewLine]", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"rmax", "=", 
       RowBox[{

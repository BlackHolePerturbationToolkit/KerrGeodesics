Cell["\<\
Plot the photo sphere radius, IBSO and ISSO for given black hole spin\
\>", "Text",
 CellChangeTimes->{{3.709551551324498*^9, 3.709551562764736*^9}, {
  3.7095516549131517`*^9, 
  3.7095516773563833`*^9}},ExpressionUUID->"6189b4c4-77c2-4134-b981-\
34780b7a03d1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PlotSpherical", "[", "a_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"rph", ",", "rmb", ",", "rms", ",", 
      RowBox[{"n", "=", "30"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"rph", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"KerrGeoPhotonSphereRadius", "[", 
           RowBox[{"a", ",", "\[Theta]"}], "]"}], ",", 
          RowBox[{"180", 
           FractionBox["\[Theta]", "\[Pi]"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]", ",", 
          FractionBox["\[Pi]", "n"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rmb", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"KerrGeoIBSO", "[", 
           RowBox[{"a", ",", "\[Theta]"}], "]"}], ",", 
          RowBox[{"180", 
           FractionBox["\[Theta]", "\[Pi]"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]", ",", 
          FractionBox["\[Pi]", "n"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"rms", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"KerrGeoISSO", "[", 
           RowBox[{"a", ",", "\[Theta]"}], "]"}], ",", 
          RowBox[{"180", 
           FractionBox["\[Theta]", "\[Pi]"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]", ",", 
          FractionBox["\[Pi]", "n"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Plot", "[", 
        RowBox[{"180", ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", 
           RowBox[{"rmb", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"rms", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}]}], "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", "None"}], ",", 
         RowBox[{"Filling", "\[Rule]", "Axis"}], ",", 
         RowBox[{"FillingStyle", "\[Rule]", "LightRed"}], ",", 
         RowBox[{"PlotTheme", "\[Rule]", "\"\<Detailed\>\""}], ",", 
         RowBox[{"Frame", "\[Rule]", "True"}], ",", 
         RowBox[{"ImageSize", "\[Rule]", "800"}], ",", 
         RowBox[{"Axes", "\[Rule]", "False"}], ",", 
         RowBox[{"BaseStyle", "\[Rule]", "20"}], ",", 
         RowBox[{"FrameLabel", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
           "\"\<r/M\>\"", ",", 
            "\"\<\!\(\*SubscriptBox[\(\[Theta]\), \(inc\)]\)\>\""}], 
           "}"}]}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Plot", "[", 
        RowBox[{"180", ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", 
           RowBox[{"rms", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}], ",", "10"}], 
          "}"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", "None"}], ",", 
         RowBox[{"Filling", "\[Rule]", "Axis"}], ",", 
         RowBox[{"FillingStyle", "\[Rule]", "LightBlue"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ListPlot", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"rph", ",", "rmb", ",", "rms"}], "}"}], ",", 
         RowBox[{"Filling", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"2", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"Axis", ",", "LightRed"}], "}"}]}], ",", 
            RowBox[{"3", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{"Axis", ",", "LightBlue"}], "}"}]}]}], "}"}]}], ",", 
         RowBox[{"Joined", "\[Rule]", "True"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"Green", ",", "Red", ",", "Blue"}], "}"}]}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PlotRange", "\[Rule]", "All"}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.709021927364081*^9, 3.709021930456918*^9}, {
